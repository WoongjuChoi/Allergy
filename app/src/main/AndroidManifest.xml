<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.allergy">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Allergy"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".MenuResister"
            android:label="@string/title_activity_menu_resister_view"
            android:theme="@style/Theme.Allergy.NoActionBar"></activity>
        <activity android:name=".EntireMenuActivity" />
        <activity android:name=".AllergyMenuActivity" />
        <activity android:name=".StoreListActivity" />
        <activity android:name=".MenuListActivity" />
        <activity android:name=".ResisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".UserInfoSetting" />
        <activity android:name=".SearchListActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>